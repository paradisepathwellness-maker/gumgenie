{
  "name": "gumgenie-pro-ai---advanced-architect",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "backend:dev": "tsx watch backend/server.ts",
    "dev:full": "npm run predev:full && concurrently -k -n FRONTEND,BACKEND -c cyan,magenta \"npm run dev\" \"npm run backend:dev\"",
    "predev:full": "node scripts/kill_ports.mjs 3110 4000",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "typecheck": "tsc --noEmit",
    "export:production-test": "tsx scripts/exportProductionTest.ts",
    "generate:production-ready": "tsx scripts/generateProductionReady.ts",
    "generate:production-ready:one": "tsx scripts/generateProductionReady.ts --category=AI_PROMPTS",
    "validate:market": "tsx scripts/marketValidationCli.ts",
    "audit:framework": "tsx scripts/frameworkAuditCli.ts",
    "validate:all": "tsx scripts/validateAllCli.ts",
    "foreman": "node scripts/foreman.mjs",
    "foreman:build": "node scripts/foreman.mjs --build",
    "validate:deep": "tsx scripts/deepDiveGumroadValidation.ts",
    "foreman:watch": "node --watch scripts/foreman.mjs"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.562.0",
    "ogl": "^1.0.11",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.1.0",
    "shadcn": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
